void sortColors(int* nums, int numsSize){
    int n=nums.size();
       int count=0,count1=0,count2=0;
       for(int i=0;i<n;i++)
       {
           if(nums[i]==0)count++;
           else if(nums[i]==1)count1++;
           else if(nums[i]==2)count2++;
       }
       for(int i=0;i<n;i++)
       {
           if(count>0)
           {
               nums[i]=0;
               count--;
           }
           else if(count1>0)
           {
               nums[i]=1;
               count1--;
           }
           else
           {
               nums[i]=2;
           }   
       }
       return ;   
   }
